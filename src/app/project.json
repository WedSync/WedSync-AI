{
  "name": "wedsync-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "src/app",
  "tags": ["scope:app", "type:nextjs"],
  "implicitDependencies": ["wedsync-components", "wedsync-lib"],
  "targets": {
    "pages-analysis": {
      "executor": "nx:run-commands",
      "options": {
        "command": "find src/app -name 'page.tsx' | wc -l"
      }
    }
  }
}